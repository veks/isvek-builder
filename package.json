{
  "name": "isvek-plugin",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "wp-build": "wp-scripts build --webpack-copy-php --webpack-src-dir=src/blocks --output-path=assets/blocks",
    "wp-watch": "wp-scripts start --webpack-src-dir=src/blocks --output-path=assets/blocks",
    "contact-forms-build": "npm-run-all --parallel contact-forms-compile",
    "contact-forms-compile": "npm-run-all --parallel contact-forms-js contact-forms-js-minify",
    "contact-forms-js": "rollup --config src/builder/rollup.contact-forms.js --bundleConfigAsCjs",
    "contact-forms-js-minify": "terser --compress passes=2 --source-map --output assets/js/contact-forms.min.js assets/js/contact-forms.js",
    "contact-forms-js-watch": "nodemon --watch ./src/blocks/contact-forms/ --ext js --exec \"npm run contact-forms-compile\"",
    "yandex-maps-js-compile": "npm-run-all --parallel yandex-maps-js yandex-maps-js-minify",
    "yandex-maps-js": "rollup --config src/builder/rollup.yandex-maps.js --bundleConfigAsCjs",
    "yandex-maps-js-minify": "terser --compress passes=2 --source-map --output assets/js/yandex-maps.min.js assets/js/yandex-maps.js",
    "yandex-maps-js-watch": "nodemon --watch ./src/blocks/yandex-maps/ --ext js --exec \"npm run yandex-maps-js-compile\"",
    "admin:settings-js-compile": "npm-run-all --parallel admin:settings-js admin:settings-js-minify",
    "admin:settings-js": "rollup --config src/builder/rollup.settings.js --bundleConfigAsCjs",
    "admin:settings-js-minify": "terser --compress passes=2 --source-map --output assets/js/settings.min.js assets/js/settings.js",
    "admin:settings-js-watch": "nodemon --watch ./src/js/ --ext js --exec \"npm run admin:settings-js-compile\"",
    "admin:settings-css-compile": "npm-run-all --parallel admin:settings-css admin:settings-css-minify",
    "admin:settings-css": "node-sass --style expanded src/scss/settings.scss assets/css/settings.css --output-style expanded --source-map true",
    "admin:settings-css-minify": "node-sass --include-path scss assets/css/settings.css assets/css/settings.min.css --output-style compressed --source-map true",
    "npm-check-updates": "npx npm-check-updates",
    "ncu-updates": "ncu -u",
    "help": "rollup --help",
    "npm-install": "npm install --legacy-peer-deps"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/helper-builder-binary-assignment-operator-visitor": "^7.22.15",
    "@babel/plugin-external-helpers": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@wordpress/api-fetch": "^6.52.0",
    "@wordpress/block-editor": "^12.23.0",
    "@wordpress/blocks": "^12.32.0",
    "@wordpress/components": "^27.3.0",
    "@wordpress/compose": "^6.32.0",
    "@wordpress/data": "^9.25.0",
    "@wordpress/element": "^5.32.0",
    "@wordpress/hooks": "^3.55.0",
    "@wordpress/icons": "^9.46.0",
    "@wordpress/scripts": "^27.6.0",
    "@wordpress/server-side-render": "^4.32.0",
    "@wordpress/shortcode": "^3.55.0",
    "bootstrap": "^5.3.3",
    "inputmask": "^5.0.8",
    "install": "^0.13.0",
    "jquery": "^3.7.1",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.18",
    "npm-run-all": "^4.1.5",
    "react-bootstrap": "^2.10.2",
    "react-hook-form": "^7.51.3",
    "react-select": "^5.8.0",
    "react-sortablejs": "^6.1.4",
    "react-yandex-maps": "^4.6.0",
    "rollup": "^4.14.3",
    "rollup-plugin-async": "^1.2.0",
    "sortablejs": "^1.15.2",
    "terser": "^5.30.3",
    "ymaps": "^0.2.1"
  },
  "dependencies": {
    "core-js": "^3.37.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.14.1"
  }
}
