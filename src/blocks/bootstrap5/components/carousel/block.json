{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 3,
  "name": "isvek-plugin-blocks/carousel",
  "version": "1.0.0",
  "title": "Слайдер",
  "icon": "slides",
  "category": "isvek-plugin-blocks",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "carouselImages": {
      "type": "array",
      "default": []
    },
    "carouselVariantDark": {
      "type": "boolean",
      "default": false
    },
    "carouselInterval": {
      "type": "number",
      "default": 5
    },
    "carouselCaption": {
      "type": "boolean",
      "default": true
    },
    "carouselIndicators": {
      "type": "boolean",
      "default": true
    },
    "carouselControls": {
      "type": "boolean",
      "default": true
    },
    "carouselFade": {
      "type": "boolean",
      "default": false
    },
    "carouselWrap": {
      "type": "boolean",
      "default": false
    },
    "carouselUrl": {
      "type": "boolean",
      "default": false
    },
    "carouselPause": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}